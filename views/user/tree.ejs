<%- include('../header'); %>
	<!-- BEGIN breadcrumb -->
	<ol class="breadcrumb float-xl-end">
		<li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
		<li class="breadcrumb-item active">계보도</li>
	</ol>
	<!-- END breadcrumb -->
	<!-- BEGIN page-header -->
	<h1 class="page-header mb-3">계보도</h1>
	<!-- END page-header -->

	<div class="row">
		<div class="col">
			<div class="panel panel-inverse">
				<div class="panel-heading">
					<h4 class="panel-title"><i class="fa fa-users me-1"></i> 계보도 <span class="badge bg-green ms-1">
							<%=tree.length%>
						</span></h4>
				</div>
				<div class="panel-body">
					<!-- html -->
					<div id="jstree-default">
						<ul>
							<li data-jstree='{"opened":true}'>
								Root node 1
								<ul>
									<li data-jstree='{"opened":true, "selected":true }'>Initially Selected</li>
									<li>Folder 1</li>
								</ul>
							</li>
						</ul>
					</div>
					<% if(!tree || tree.length==0) { %>
						<div>데이터가 없습니다</div>
						<%} else { tree.forEach(function(e, i) { let email=e.f_email; %>
							<%=email%>
								<%})%>
									<%}%>

				</div>
			</div>
		</div>
	</div>
	<!-- required files -->
	<link href="/assets/plugins/jstree/style.min.css" rel="stylesheet" />
	<script src="/assets/plugins/jstree/jstree.min.js"></script>



	<script>
		$("#jstree-default").jstree({
			"plugins": ["types"],
			"core": {
				"themes": { "responsive": false }
			},
			"types": {
				"default": { "icon": "fa fa-folder text-warning fa-lg" },
				"file": { "icon": "fa fa-file text-dark fa-lg" }
			}
		});
	</script>
	<%- include('../footer'); %>